{
    "name": "treehousecapstone",
    "version": "0.0.0",
    "private": true,
    "engines": {
      "node": "8.0.0",
      "npm": "5.4.1"
    },
    "scripts": {
        "start": "node ./bin/www",
        "server": "nodemon ./bin/www",
        "client": "npm start --prefix client",
        "dev": "concurrently \"npm run client\" \"npm run server\"",
        "heroku-postbuild": "NODE_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
    },
    "dependencies": {
        "body-parser": "~1.17.1",
        "concurrently": "^3.5.0",
        "connect-mongo": "^1.3.2",
        "cookie-parser": "~1.4.3",
        "debug": "~2.6.3",
        "express": "~4.15.2",
        "express-session": "^1.15.5",
        "jsonwebtoken": "^8.0.1",
        "mongoose": "^4.11.12",
        "morgan": "~1.8.1",
        "passport": "^0.4.0",
        "passport-twitter": "^1.0.4",
        "pug": "^2.0.0-rc.4",
        "serve-favicon": "~2.4.2",
        "twit": "^2.2.9"
    }
}
